<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.practise1">

    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.SEND_SMS" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/practise"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/practise_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Practise1"
        android:usesCleartextTraffic="true">
        <activity
            android:name=".EmailActivity"
            android:exported="false" />
        <activity
            android:name=".SendSms"
            android:exported="false" />
        <activity
            android:name=".splashoone"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".splashScreen"
            android:exported="true" />
        <activity
            android:name=".MainActivity"
            android:exported="false" />
        <activity
            android:name=".recyclerview"
            android:exported="false" />
        <activity
            android:name=".ScrollView"
            android:exported="false" />
        <activity
            android:name=".GridView"
            android:exported="false" />
        <activity
            android:name=".WebView"
            android:exported="false" />
        <activity
            android:name=".listview"
            android:exported="false" />
    </application>

</manifest>